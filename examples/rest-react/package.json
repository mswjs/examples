{
  "name": "rest-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@playwright/test": "1.17.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "msw": "0.36.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "tslib": "2.3.1",
    "typescript": "4.5.3",
    "web-vitals": "2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
